class Solution {
public:
    int numTrees(int n) {
      int dp[n+1];//0-n
      dp[0]=1;
      dp[1]=1;  
     // int temp=0; 
        for(int i=2;i<=n;i++)
        {
            int temp=0;
            int k=i;
            while(k>0)
            {
               temp+=dp[i-k]*dp[k-1];
               k--; 
            }
            dp[i]=temp;
        }
        return dp[n];


    }
};
